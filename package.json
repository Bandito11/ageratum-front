{
  "name": "banditotr-blog",
  "private": true,
  "version": "0.10.10",
  "description": "My personal blog",
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --prod --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "@stencil/core": "0.10.10",
    "@stencil/helmet": "^0.2.5",
    "@stencil/router": "^0.2.5"
  },
  "devDependencies": {
    "@types/jest": "23.3.1",
    "jest": "^23.4.2",
    "request": "^2.87.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bandito11/banditotr-front.git"
  },
  "bugs": {
    "url": "https://github.com/Bandito11/banditotr-front"
  },
  "homepage": "https://www.banditotr.com",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
